<h1>Login</h1>
<%= form_with url: login_path, local: true do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :email %>  <!-- Removed :session -->
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :password %>  <!-- Removed :session -->
  </div>
  <div>
    <%= form.submit "Login" %>
  </div>
<% end %>

