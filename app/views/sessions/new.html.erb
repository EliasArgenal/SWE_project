<h1>Login</h1>
<%= form_with url: login_path, local: true do |form| %>
  <div>
    <%= form.label :email %>
    <%= form.email_field :session, :email %>
  </div>
  <div>
    <%= form.label :password %>
    <%= form.password_field :session, :password %>
  </div>
  <div>
    <%= form.submit "Login" %>
  </div>
<% end %>
